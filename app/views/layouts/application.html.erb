<!DOCTYPE html>
<!--[if IE 9]> <html lang="ru" class="js-no ie9"> <![endif]-->
<!--[if IE 10]> <html lang="ru" class="js-no ie10"> <![endif]-->
<!--[if !IE]><!-->
<html lang="ru">
<!--<![endif]-->

<head>
    <title>Центр Инноваций Ай-Теко</title>

    <!-- Meta Tags -->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Центр инноваций Ай-Теко">
    <meta name="keywords" content="Ай-Теко">
    <meta name="author" content="Ай-Теко">
		
		<!--===    STYLES     ===-->
		<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <!--  Boostrap Framework  -->
    <!-- <link href="assets/css/bootstrap.min.css" rel="stylesheet"> -->

    <!--=== CSS - Dragonfly ===-->
    <!-- <link href="assets/css/themes/light-blue.css" rel="stylesheet" id="colors"> -->

    <!--=== LESS - Dragonfly ===-->
    <!--<link href="assets/less/main.less" rel="stylesheet/less">-->

    <!-- Google Fonts - Lato -->
    <link href="http://fonts.googleapis.com/css?family=Lato:300,400" rel="stylesheet">

    <!-- Font Awesome Icons -->
    <!-- <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet"> -->

    <!-- CSS Animations -->
    <link href="http://cdnjs.cloudflare.com/ajax/libs/animate.css/3.1.1/animate.min.css" rel="stylesheet">

    <!--  Slippry Slideshow -->
    <!-- <link href="assets/css/slippry.css" rel="stylesheet"> -->
		<!--===/END    STYLES     ===-->
		
    <!--===    SCRIPTS     ===-->
	  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag "bootstrap-select/i18n/defaults-#{bootstrap_select_locale}", 'data-turbolinks-track' => true %>
    <% if content_for?(:script) %>
      <script type="text/javascript"><%= yield(:script) %></script>
    <% end %>
    <!-- Modernizr -->
    <!--  <script src="assets/js/min/modernizr.custom.min.js"></script> -->

    <!-- jQuery -->
    <!-- <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script> -->
    <!-- <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.10.4/jquery-ui.min.js"></script> -->

    <!-- Bootstrap JS -->
    <!-- <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script> -->

    <!-- Bootstrap Plugin - open dropdown on hover -->
    <!-- <script src="assets/js/min/bootstrap-hover-dropdown.min.js"></script> -->

    <!-- LESS preprocessor -->
    <script src="http://cdnjs.cloudflare.com/ajax/libs/less.js/1.7.4/less.min.js"></script>

    <!-- WOW.js - loading animations -->
    <script src="http://cdnjs.cloudflare.com/ajax/libs/wow/0.1.6/wow.min.js"></script>

    <!-- Knobs - our skills -->
    <script src="http://cdn.jsdelivr.net/jquery.knob/1.2.9/jquery.knob.min.js"></script>

    <!-- Mixitup plugin - Portfolio Filter Grid -->
    <script src="http://cdnjs.cloudflare.com/ajax/libs/mixitup/1.5.6/jquery.mixitup.min.js"></script>

    <!-- Make sticky whatever elements -->
    <script src="http://cdn.jsdelivr.net/jquery.sticky/1.0.0/jquery.sticky.min.js"></script>

    <!-- Smooth sroll -->
    <script src="http://cdn.jsdelivr.net/jquery.nicescroll/3.5.4/jquery.nicescroll.min.js"></script>

    <%= javascript_tag do %>
      $(document).ready(function() {
        $('.selectpicker').selectpicker({
          'selectedText': 'cat'
        });
        $('input.bootstrap-switch').bootstrapSwitch();
        <% if content_for?(:ready_script) %>
          <%= yield(:ready_script) %>
        <% end %>
      });
    <% end %>
		
    <!-- Contact Form -->
    <!--  <script src="assets/js/min/contact-form.min.js"></script> -->

    <!-- Slippry - Slideshow -->
    <!-- <script src="assets/js/min/slippry.min.js"></script> -->
    <%= javascript_include_tag 'min/slippry.min', 'data-turbolinks-track' => true %>
		
    <!-- Must be last of all scripts -->
    <!-- <script src="assets/js/min/scripts.min.js"></script> -->
		<%= javascript_include_tag 'min/scripts.min', 'data-turbolinks-track' => true %>
		
    <!--[if lt IE 9]>
          <script src="http://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7/html5shiv.min.js"></script>
          <script src="http://cdnjs.cloudflare.com/ajax/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    
    <!--===/END    SCRIPTS     ===-->		
	  <%= csrf_meta_tags %>

</head>
<body>
    <!--===    NAVBAR     ===-->
    <nav class="navbar navbar-trn navbar-fixed-top" role="navigation" id="navbarSettings">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="http://i-teco.ru"><%= image_tag "i-teco_invert.png", :style => "width: 150px; top: 22px; position:absolute;" %></a>
            </div>

            <div class="collapse navbar-collapse" id="bs-navbar-collapse-1">
                <ul class="nav navbar-nav navbar-right ">
                    <!--Home Pages -->
                    <li><%= link_to "Главная", root_path %></li>

                    <!--Mission-->
                    <li><%= link_to "Миссия", "/#mission", :class => "scroll" %></li>

                    <!--Get Started-->
                    <li><%= link_to "С чего начать", "/#start", :class => "scroll" %></li>
										
                    <!--Priority-->
                    <li><%= link_to "Приоритеты", "/#priority", :class => "scroll" %></li>


                    <!-- Sidebar -->
                    <li class="dropdown hidden-xs">
                        <a href="javascript:void(0);" id="sidebar-btn">
                            <i class="fa fa-bars"></i>
                        </a>
                    </li>

                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </div>
    </nav>
    <!-- If navbar is fixed to top and it has the class navbar-default or navbar-inverse this div will add space below the navbar-->
    <div id="navbarSpaceBottom"></div>
    <!--===/END    Navbar     ===-->
    <% if request.format == Mime::HTML && !flash.empty? %>
      <%= render "layouts/flash", :flash => flash %>
    <% end %>
    <%= yield %>

    <!--Back to top-->
    <a href="#" class="back-to-top">
        <i class="fa fa-angle-up"></i>
    </a>

    <% if content_for?(:switcher) %>
	    <!--===    STYLE SWITCHER     ===-->
	    <div class="style-switcher">
	        <i class="fa fa-gear switcher-icon"></i>
	        <div class="body">
						<h4>Действия</h4>
						<div class="resource-actions">
							  <%= yield(:switcher) %>
						</div>
	        </div>
	    </div>
	    <!--===/END    STYLE SWITCHER     ===-->
		<% end %>

    <!--===    SIDEBAR     ===-->
    <div class="sidebar-off hidden-xs sidebar">
        <div class="wrap">
            <div class="space-sm"></div>
            <h4>Эккаунт</h4>
                <ul class="info-list">
							  <% if user_signed_in? && Rails.env.development? %>
								  <li>
										<i class="fa fa-user"></i>
								    <%= link_to "Учетная запись", user_url(current_user), :class => "text-muted" %>
								  </li>
									<li>
										<%= button_to destroy_user_session_path, :class => "btn btn-default btn-xs", :method => :delete do %>
										  <i class="fa fa-sign-out"></i> Выйти
										<% end %>
									</li>
								<% else %>
								  <li>
										<i class="fa fa-sign-in"></i>
								     <%= link_to "Логин", new_user_session_path, :class => "text-muted" %>
									</li>
									<li class="divider"></li>
									<li>
										<i class="fa fa-pencil"></i>
									  <%= link_to "Регистрация", new_user_registration_path, :class => "text-muted" %>
									</li>
							  <% end %>
              </ul>
	          <div class="space"></div>
						
            <h4>Языки</h4>
            <ul class="info-list sm-list lang">
                <li class="active">
                    <i class="fa fa-angle-right fa-fw"></i>
                    <a href="#">Русский</a>
                    <i class="fa fa-check fa-fw"></i>
                </li>
            </ul>

            <div class="space"></div>

            <h4>Соцмедиа</h4>
            <ul class="icons-list circle-icons m-no-bottom">
                <%= render "layouts/social_links" %>
            </ul>

            <div class="space"></div>

            <h4>Контакты</h4>
            <ul class="info-list sm-list">
                <%= render "layouts/contacts" %>
            </ul>
        </div>
    </div>
    <!--===/END    SIDEBAR     ===-->

    <% unless controller?(%w{ devise/sessions devise/passwords devise/registrations uploads pictures}) %>
		    <!--===    FOOTER     ===-->
		    <footer class="footer">
		        <div class="footer-top">
		            <div class="container">
		                <div class="row">
		                    <div class="col-md-4 col-lg-4 col-sm-12 col-xs-12">
		                        <h4>Контакты</h4>
		                        <ul class="info-list md-list i-primary">
		                            <%= render "layouts/contacts" %>
		                        </ul>
		                    </div>
		                    <div class="space-sm visible-sm visible-xs"></div>
		                    <div class="col-md-4 col-lg-4 col-xs-12">
		                        <h4>Ссылки</h4>
		                        <ul class="info-list md-list i-primary">
		                            <li>
		                                <i class="fa fa-angle-right fa-fw"></i>
		                                <a href="/#mission" class="scroll">
		                                    <span class="link-title">Миссия</span></a>
		                            </li>
		                            <li>
		                                <i class="fa fa-angle-right fa-fw"></i>
		                                <a href="/#start" class="scroll">
		                                    <span class="link-title">Следуйте за нами</span></a>
		                            </li>
		                            <li>
		                                <i class="fa fa-angle-right fa-fw"></i>
		                                <a href="/#priority" class="scroll">
		                                    <span class="link-title">Наши приоритеты</span></a>
		                            </li>
		                            <li>
		                                <i class="fa fa-angle-right fa-fw"></i>
		                                <a href="http://i-teco.ru/company/">
		                                    <span class="link-title">О компании</span></a>
		                            </li>
		                        </ul>
		                    </div>
		                    <div class="space-sm visible-sm visible-xs"></div>
		                    <div class="col-md-4 col-lg-4  col-xs-12">
		                        <h4>О нас</h4>
		                        <p><a href="http://i-teco.ru" style="font-weight:bold;">Группа компаний "Ай-Теко"</a><br>Центр Инноваций</p>
		                        <div class="space-sm"></div>
		                        <h4>Следите за нами</h4>
		                        <ul class="circle-icons icons-list">
		                            <%= render "layouts/social_links" %>
		                        </ul>
		                    </div>
		                </div>
		            </div>
		        </div>

		        <div class="footer-bottom">
		            <div class="container">
		                <p>© 2014&nbsp;&nbsp;ЗАО "Ай-Теко"</p>
		            </div>
		        </div>
		    </footer>
		    <!--===/END    FOOTER     ===-->
		<% end %>
    
</body>
</html>